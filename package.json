{
  "name": "server_manager",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"vite\" \"npx steiger ./src --watch\"",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier src/ \"!src/modules/openapi/api.*\" --write",
    "openapi": "npx tsx src/modules/openapi/generate.ts && npm run openapi:format",
    "openapi:format": "prettier --write src/modules/openapi/api.* && npm run lint:fix",
    "openapi:download": "npx tsx src/modules/openapi/generate.ts --mode download",
    "openapi:generate": "npx tsx src/modules/openapi/generate.ts --mode generate"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.3",
    "@tanstack/vue-query": "^5.89.0",
    "@tsconfig/node20": "^20.1.6",
    "@vueuse/core": "^13.9.0",
    "include-media": "^2.0.0",
    "ky": "^1.10.0",
    "minimist": "^1.2.8",
    "openapi-typescript": "^7.9.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.9",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@feature-sliced/steiger-plugin": "^0.5.7",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.92.1",
    "steiger": "^0.5.11",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  }
}
